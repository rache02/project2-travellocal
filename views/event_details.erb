<h1>More information</h1>

<a href="/events/<%= @event['id']%>/edit">edit</a>

<form class="" action="/events/<%= @event['id'] %>" method="post">
  <input type="hidden" name="_method" value="delete">
  <button>delete</button>
</form>

<h2><%= @event['name']%></h2>
